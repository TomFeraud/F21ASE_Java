package tests;

import static org.junit.Assert.*;

import org.junit.Before;
import org.junit.Test;

import F21ASE_Stage1.Passenger;

public class TestMyPassenger {

	private Passenger passenger1;
	private Passenger passenger2;

	@Before
	public void setUp() {
		passenger1 = new Passenger("Jean", "Michel", "Saumon");
		passenger2 = new Passenger("Pierre Yves Lafonte");
	}

	@Test
	public void testgetFirstName() {
		String firstName = "Pierre";
		String testMethod = passenger1.getFirstName();
		System.out.println(passenger1.getFirstName());
		assertTrue(firstName.equals(testMethod));

	}

}
